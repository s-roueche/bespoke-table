{
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "endOfLine": "auto",
  "importOrder": [
    "^react",
    "^(?!(hooks|types|utils|shared|modules|[./])).+",
    "^hooks",
    "^types",
    "^utils",
    "^shared",
    "modules",
    "^[./]"
  ],
  "importOrderSeparation": false
}
